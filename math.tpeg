DIGIT = [0-9]
Value = { DIGIT+  #int }  //{}で囲んだ部分は構文木のノードになる #intはノードの名前
Zip = {                   //郵便番号の定義
    Value '-' Value
    #Zip
}

Add = {
    Value '+' Value
    #Add
}
// Sub = {
//     Value '-' Value
//     #Sub
// }
Mul = {
    Value '*' Value
    #Mul
}
Div = {
    Value '/' Value
    #Div
}
